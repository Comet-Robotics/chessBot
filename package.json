  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@vitejs/plugin-react": "^3.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.7",
    "vite": "^4.0.4"
